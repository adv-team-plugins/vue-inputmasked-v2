{
  "name": "vue-inputmasked",
  "version": "1.3.2",
  "description": "Super tiny input mask library for Vue.js based on PureMask.js (~2kb) exposed as directive. No dependencies",
  "main": "dist/vue-inputmasked.js",
  "module": "dist/vue-inputmasked.esm.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "npm run bundle && npm run bundle:min",
    "bundle": "rollup -c build/rollup.conf.js",
    "bundle:min": "rollup -c build/rollup.conf.min.js",
    "release": "bash scripts/release.sh",
    "release:note": "node scripts/gen-release-note.js",
    "lint": "eslint src scripts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pma87/vue-inputmasked.git"
  },
  "keywords": [
    "vue.js",
    "vue",
    "vue-mask"
  ],
  "author": "Max Lyashuk <pma987@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/pma87/vue-inputmasked.git#readme",
  "devDependencies": {
    "@vue/test-utils": "1.0.0-beta.13",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "conventional-changelog": "1.1.23",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jest": "21.15.0",
    "jest": "22.4.3",
    "rollup": "0.57.1",
    "rollup-plugin-babel": "3.0.3",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-uglify": "3.0.0",
    "vue": "2.5.16",
    "vue-server-renderer": "2.5.16",
    "vue-template-compiler": "2.5.16"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ]
  }
}
